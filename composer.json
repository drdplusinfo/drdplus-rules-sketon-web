{
  "name": "drdplus/rules-skeleton-web",
  "license": "MIT",
  "type": "library",
  "description": "Content for DrD+ rules skeleton",
  "keywords": [
    "drd plus",
    "rules",
    "helper",
    "skeleton",
    "content"
  ],
  "autoload": {
    "psr-4": {
      "DrdPlus\\": "DrdPlus/"
    }
  },
  "require": {
    "ext-dom": "*",
    "ext-yaml": "*",
    "granam/strict-object": "^3.0",
    "granam/string": "^3.2",
    "phpgt/dom": "^1.1",
    "granam/exceptions-hierarchy": "^4.0",
    "granam/yaml-reader": "^1.0",
    "granam/web-content-builder": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.3",
    "mockery/mockery": "^1.2"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
